{
  "manifest_version": 3,
  "name": "Accurate Geolocation",
  "version": "1.0",
  "description": "Get the user's accurate geolocation",
  "permissions": ["geolocation", "downloads", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
